{
  "manifest_version": 2,
    "name": "jQuery Extract InnerText",
    "version": "3",
    "description": "Extraer texto de clases especificas en HTML",
  "icons": {
    "48": "icon.png"
  },

  "permissions": [
    "tabs", "<all_urls>" , "activeTab" , "downloads" , "downloads.open"
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}